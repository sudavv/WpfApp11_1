<Window x:Class="WpfApp11_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp11_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <Path Fill="Red" Stroke="Black" VerticalAlignment="Center" HorizontalAlignment="Center" Stretch="Fill" Height="350" Width="350">

            <Path.Data>
                <EllipseGeometry Center="0,0" RadiusX="30" RadiusY="40"/>
            </Path.Data>

        </Path>


        <Path Fill="White" Stroke="Black" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="284.66,113.695,250.339,60.304">

            <Path.Data>
                <PathGeometry FillRule="Nonzero">

                    <PathFigure StartPoint="0,200" IsClosed="True">
                        <ArcSegment Point="250,50" Size="150,150"/>
                    </PathFigure>

                    <PathFigure StartPoint="-30,150" IsClosed="True">
                        <ArcSegment Point="225,0" Size="150,150" SweepDirection="clockwise"/>
                    </PathFigure>
                </PathGeometry>
            </Path.Data>
        
        </Path>

       

    </Grid>
</Window>
